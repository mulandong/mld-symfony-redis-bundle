{
    "name": "mulandong/mld-symfony-redis-bundle",
    "type": "symfony-bundle",
    "description": "A Redis bundle for Symfony",
    "keywords": ["redis", "nosql", "symfony"],
    "homepage": "https://github.com/mulandong/mld-symfony-redis-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "mulandong",
            "email": "mulan_dong@163.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "^2.7 || ^3.0",
        "symfony/yaml": "^2.7 || ^3.0"
    },
    "require-dev": {
        "doctrine/cache": "1.*",
        "predis/predis": "1.1.1",
        "symfony/console": "^2.7 || ^3.0",
        "symfony/phpunit-bridge": "^2.7 || ^3.0",
        "phpunit/phpunit": "4.8.*"
    },
    "suggest": {
        "monolog/monolog": "If you want to use the monolog redis handler.",
        "predis/predis": "If you want to use predis.",
        "symfony/console": "If you want to use commands to interact with the redis database"
    },
    "autoload": {
        "psr-4": { "Snc\\RedisBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
